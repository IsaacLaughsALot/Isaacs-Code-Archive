        me2.CFrame =  me2.CFrame * CFrame.Angles(math.rad(0), math.rad(0), math.rad(2))
        smok.Size = smok.Size + 10
        wait()
    end
    S4:remove()
    pe:remove()
    pe2:remove()
    bfor:remove()
    me2:remove()
    me:remove()
    for i = 0,1,0.01 do
        wait()
        smok.Opacity = smok.Opacity - 0.01
    end
    part:remove()
    sound2:remove()
    sound3:remove()
end
 
